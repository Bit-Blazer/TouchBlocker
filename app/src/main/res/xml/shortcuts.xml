<?xml version="1.0" encoding="utf-8"?>
<shortcuts xmlns:android="http://schemas.android.com/apk/res/android">
  <shortcut
      android:shortcutId="enable_touch_blocker"
      android:enabled="true"
      android:icon="@drawable/lock_24px"
      android:shortcutShortLabel="@string/shortcut_enable_label"
      android:shortcutLongLabel="@string/shortcut_enable_long_label">
    <intent
        android:action="android.intent.action.VIEW"
        android:targetPackage="com.bitblazer.touchblocker"
        android:targetClass="com.bitblazer.touchblocker.NoDisplayActivity">
      <extra android:name="enable_blocker" android:value="true" />
    </intent>
  </shortcut>
</shortcuts>
